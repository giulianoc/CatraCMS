{
    "type": "Workflow",
    "label": "1",
    "Variables": {
        "TitleOfVideo": "My Title of the video content"
    },
    "Task": {
        "label": "My video (push)",
        "type": "Add-Content",
        "Parameters": {
            "FileFormat": "mp4",
            "UserData": { "MyData": "this is my video data" },
            "Title": "${TitleOfVideo}",
	    "FileSizeInBytes": 278341022
        },
        "OnSuccess": {
            "Task": {
                "label": "my label",
                "type": "Encode",
                "Parameters": {
                    "EncodingPriority": "Low",
                    "EncodingProfilesSetLabel": "Sport"
                }
            }
        }
    }
}
