{
	"type": "Workflow",
	"label": "1",
	"Variables": {
		"TitleOfVideo": "My Title of the video content"
	},
	"task": {
		"label": "My label ...",
		"type": "Add-Content",
		"parameters": {
			"SourceURL": "http://techslides.com/demos/sample-videos/small.mp4",
			"FileFormat": "mp4",
			"userData": { "MyData": "this is my video data" },
			"Title": "${TitleOfVideo}"
		},
		"OnSuccess": {
			"task": {
				"type": "Remove-Content",
				"parameters": {
				}
			}
		}
	}
}
